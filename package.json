{
  "name": "bp-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "start:prod": "cross-env NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "prestart:prod": "cross-env NODE_ENV=production  rimraf dist && tsc",
    "build": "cross-env NODE_ENV=production  tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.7.2",
    "@types/nodemailer": "^6.2.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.1",
    "mongoose-auto-increment": "^5.0.1",
    "mongoose-paginate": "https://github.com/surmon-china/mongoose-paginate",
    "nodemailer": "^6.3.0",
    "qiniu": "^7.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "typegoose": "^5.9.1",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.1",
    "@types/express-rate-limit": "^3.3.3",
    "@types/helmet": "0.0.44",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.136",
    "@types/marked": "0.6.5",
    "@types/mongodb": "^3.3.3",
    "@types/mongoose": "^5.5.9",
    "@types/mongoose-auto-increment": "^5.0.31",
    "@types/mongoose-paginate": "^5.0.7",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "cross-env": "^6.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
